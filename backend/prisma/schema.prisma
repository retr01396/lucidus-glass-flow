generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Registration {
  id               Int      @id @default(autoincrement())
  teamName         String
  eventName        String
  numberOfMembers  Int
  paymentScreenshot String?
  createdAt        DateTime @default(now())
}

